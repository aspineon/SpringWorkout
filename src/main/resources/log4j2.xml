<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
	<Appenders>
		<Console name="STDOUT" target="SYSTEM_OUT">
			<PatternLayout pattern="%d{dd-MM-yyyy} %-5p [%t] %C{2} (%F:%L) - %m%n" />
		</Console>
	</Appenders>
	<Loggers>
		<Root level="INFO">
			<AppenderRef ref="STDOUT" />
		</Root>
		<Logger name="com.springWorkout.service" level="INFO"/>
		<Logger name="org.hibernate.SQL" level="INFO"/>
	</Loggers>
</Configuration>